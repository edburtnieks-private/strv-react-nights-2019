{
  "root": true,
  "extends": [
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/jsx-filename-extension": [2, {
      "extensions": [".js", ".jsx"]
    }],
    "consistent-return": 0,
    "camelcase": 0,
    "react/state-in-constructor": [2, "never"],
    "react/no-render-return-value": 0
  }
}
